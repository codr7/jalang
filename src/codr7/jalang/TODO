* hook up type hierarchy

var dropPrefix = 0;
          var dropSuffix = 0;

          for (var v = registers[o.rTarget];;) {
            if (v.type() == Core.pairType) {
              final var p = v.as(Core.pairType);

              if (p.left().type() == Core.noneType) {
                if (target == null) {
                  dropPrefix++;
                } else {
                  dropSuffix++;
                }
              } else {
                target = p.left();
              }

              v = p.right();
            } else {
              if (v.type() == Core.noneType) {
                dropSuffix++;
              } else {
                target = v;
              }

              break;
            }
          }